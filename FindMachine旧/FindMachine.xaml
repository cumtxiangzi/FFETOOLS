<Window x:Class="FFETOOLS.FindMachineWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FFETOOLS"
        mc:Ignorable="d"
        Title="查找设备" Height="180" Width="200" ResizeMode="NoResize" Loaded="Window_Loaded" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="请输入设备编号" HorizontalAlignment="Left" Height="30" Margin="38,10,0,0" VerticalAlignment="Top" Width="116" FontSize="14"/>
        <TextBox x:Name="Note" HorizontalAlignment="Left" Height="23" Margin="46,45,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="96" FontSize="14"/>
        <Button x:Name="OkButton" Content="确定" HorizontalAlignment="Left" Height="23" Margin="56,88,0,0" VerticalAlignment="Top" Width="73" FontSize="14" Click="OkButton_Click"/>

    </Grid>
</Window>
