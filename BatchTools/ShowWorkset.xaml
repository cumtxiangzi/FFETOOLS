<Window x:Class="FFETOOLS.UserMajor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FFETOOLS"
        mc:Ignorable="d"
        Title="批量隐藏外专业工作集" Height="350" Width="250" ResizeMode="NoResize" Loaded="Window_Loaded" WindowStartupLocation="CenterScreen">
    <Grid>
        <Button x:Name="OkButton" Content="确定" HorizontalAlignment="Left" Height="29" Margin="81,129,0,0" VerticalAlignment="Top" Width="79" Click="OkButton_Click"/>
        <ComboBox x:Name="MajorNameCombo" HorizontalAlignment="Left" Height="25" Margin="56,76,0,0" VerticalAlignment="Top" Width="130"/>
        <GroupBox Header="使用说明" HorizontalAlignment="Left" Height="105" Margin="15,182,0,0" VerticalAlignment="Top" Width="208">
            <TextBlock HorizontalAlignment="Left" Height="55" Margin="15,15,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="175" Text="批量隐藏外专业工作集，要求视图名称包含专业名称，否侧会造成隐藏失败。"/>
        </GroupBox>
        <TextBlock HorizontalAlignment="Center" Margin="57,33,61,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="126" FontSize="16" TextAlignment="Center"><Run Text="请选择"/><Run Text="使用专业"/></TextBlock>

    </Grid>
</Window>
